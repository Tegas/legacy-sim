{
  "name": "react-redux-webpack2-boilerplate",
  "version": "0.1.7",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server",
    "dev": "webpack-dashboard -t 'Marvin' -- webpack-dev-server",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "lint-break-on-errors": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx",
    "lint": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx || true",
    "preview": "rm -rf ./build && NODE_ENV=\"production\" webpack-dashboard -t 'Preview Mode - Marvin' -- webpack-dev-server",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "upgrade": "node node_modules/npm-check-updates/bin/ncu -u --semverLevel major && npm prune && npm install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.1",
    "babel-plugin-syntax-decorators": "6.13.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.25.0",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "3.14.0-0",
    "npm-check-updates": "2.12.1",
    "postcss-loader": "1.3.3",
    "prepush": "3.1.11",
    "redux-form": "7.0.3",
    "redux-logger": "2.10.2",
    "sass-loader": "4.1.1",
    "style-loader": "0.18.2",
    "svg-sprite-loader": "2.1.0",
    "svgo": "0.7.2",
    "svgo-loader": "1.2.1",
    "url-loader": "0.5.9",
    "webpack": "2.7.0",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "2.6.1"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "css-loader": "^0.28.4",
    "es6-promise": "3.3.1",
    "gh-pages": "^1.0.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-redux": "4.4.8",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "recharts": "1.0.0-alpha.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "style-loader": "^0.18.2"
  },
  "description": "Legacy Simulator",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Tegas/legacy-sim.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack2",
    "boilerplate"
  ],
  "author": "Tegas",
  "bugs": {
    "url": "https://github.com/Tegas/legacy-sim/issues"
  },
  "homepage": "https://github.com/Tegas/legacy-sim",
  "prepush": [
    "npm run lint-break-on-errors --silent"
  ]
}
